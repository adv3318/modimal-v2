{
  "name": "modimal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{js,jsx} && stylelint src/**/*.{scss,css}",
    "prettier": "prettier --write src/**/*.{js,jsx,scss,css,html}",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,html,scss,css}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "devDependencies": {
    "@atrocityz/vite-plugin-html-img-to-picture": "^1.0.5",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.20.0",
    "eslint": "^9.20.1",
    "fast-glob": "^3.3.3",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.2",
    "postcss-preset-env": "^10.1.5",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.5.3",
    "sass": "^1.85.0",
    "stylelint": "^16.21.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "svgo": "^4.0.0",
    "vite": "^6.3.5",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-html-inject": "^1.1.2",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "dependencies": {
    "@a1rth/css-normalize": "^1.2.4",
    "swiper": "^12.0.3"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "not dead"
  ]
}
